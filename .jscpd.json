{
  "threshold": 5,
  "reporters": ["console", "json", "html"],
  "ignore": [
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/*.d.ts",
    "**/__tests__/**",
    "**/*.test.ts",
