{
  "name": "@nexus-state/devtools",
  "version": "0.1.4",
  "main": "index.ts",
  "types": "./src/index.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "production": "./src/devtools-noop.ts",
      "development": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -w",
    "test": "vitest run",
    "test:production": "vitest run src/__tests__/production-noop.test.ts",
    "test:performance": "vitest run src/__tests__/performance --reporter=verbose",
    "test:benchmark": "vitest bench src/__tests__/performance",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@nexus-state/core": "*"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "vitest": "^0.34.0",
    "eslint": "^8.40.0"
  }
}